<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" elementFormDefault="qualified" attributeFormDefault="unqualified">
	<xs:include schemaLocation="Types.xsd"/>
	<xs:element name="Contract">
		<xs:annotation>
			<xs:documentation>Comment describing your root element</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element name="PersonInvolved" type="link" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element name="OrganisationInvolved" type="link" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element name="Subject" type="xs:string" minOccurs="0"/>
				<xs:element ref="Type" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element name="Description" type="xs:string" minOccurs="0"/>
				<xs:element ref="Date" minOccurs="0"/>
				<xs:element ref="OfficialDocument" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="admin" minOccurs="0"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="Type" type="term"/>
</xs:schema>
